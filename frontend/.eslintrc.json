{
	"parser": "babel-eslint",
	"extends": "google",
	"env": {
		"browser": true,
		"jquery": true,
		"node": true
	},
	"globals": {
		"__DEV__": true,
		"CryptoJS": true,
		"Dropzone": true
	},
	"rules": {
		"no-confusing-arrow": 0,
		"no-tabs": 1,
		"react/jsx-quotes": 0,
		"no-new-require":2,
		"no-mixed-spaces-and-tabs": 0,
		"no-multi-spaces": 0,
		"require-jsdoc": 0,
		"arrow-parens": 0,
		"max-len": 0,
		"indent": ["error", "tab"],
		"jsx-quotes": [2, "prefer-double"]
	}
}
